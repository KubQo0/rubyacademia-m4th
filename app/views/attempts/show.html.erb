<%= render "shared/link_back", path: test_attempts_path, text: "<-- #{t(".back")}" %>
<%= render @attempt %>
<br>
<%= render @test %>

<% if current_user.admin? %>
  <div class="d-flex justify-content-between">
    <%= link_to t(".edit"), edit_test_attempt_path(@test, @attempt), class: 'btn btn-outline-primary' %>
    <%= button_to t(".destroy"), test_attempt_path(@test, @attempt), method: :delete, class: 'btn btn-outline-danger' %>
  </div>
<% end %>
