<%= render "shared/link_back", path: tests_path, text: "<-- tests" %>
<div class="d-flex justify-content-between align-items-center w-100">
  <h1>Attempts</h1>
  <%= link_to "New attempt", new_test_attempt_path, class: 'btn btn-primary' %>
</div>

<div id="attempt">
  <% @attempts.each do |attempt| %>
    <%= link_to test_attempt_path(@test, attempt), class: 'text-decoration-none text-black' do %>
      <%= render attempt %>
    <% end %>
  <% end %>
</div>
